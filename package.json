{
  "name": "csgo",
  "private": true,
  "scripts": {
    "update:version": "lerna version --no-push",
    "reinstall": "lerna clean --yes && yarn",
    "compose:prod": "docker-compose up --build -d",
    "compose:dev": "docker-compose -f docker-compose.dev.yaml -p csgo_dev up --build -d"
  },
  "workspaces": [
    "packages/*",
    "services/*"
  ],
  "devDependencies": {
    "lerna": "^3.20.2"
  }
}
